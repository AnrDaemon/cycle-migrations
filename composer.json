{
  "name": "cycle/migrations",
  "type": "library",
  "description": "Cycle ORM Migration generation",
  "require": {
    "php": "^7.1|^8.0",
    "cycle/schema-builder": "dev-master",
    "spiral/migrations": "^2.0"
  },
  "require-dev": {
    "spiral/annotations": "dev-master",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "spiral/debug": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Migrations\\Tests\\": "tests/Migrations/"
    }
  }
}
