{
  "name": "spiral/migrations",
  "description": "Migrations mechanism for spiral framework built at top of schema declarations",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "spiral/framework": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": [
        "tests/",
        "tests/cases/"
      ]
    }
  }
}
